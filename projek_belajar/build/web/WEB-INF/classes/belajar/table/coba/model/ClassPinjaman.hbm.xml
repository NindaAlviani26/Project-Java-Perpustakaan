<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="belajar.table.coba.model.ClassPinjaman" table="pinjaman">
    <id name="id_pinjaman" column="id_pinjaman">
        <generator class="assigned"></generator>
    </id>
    <property name="id_anggota" column="id_anggota"></property>
    <property name="id_buku" column="id_buku"></property>
    <property name="tgl_pinjam" column="tgl_pinjam"></property>
    <property name="tgl_kembali" column="tgl_kembali"></property>
</class>
  

</hibernate-mapping>
